#include<iostream>
struct point
{
        int x;
        int y;
};

void DrawLine(point start, point end)
{
        std::cout << "start point:" << start.x << " " << start.y << std::endl;
        std::cout << "end point:" << end.x << " " << end.y << std::endl;
}
int main()
{
        point p1 = {10, 20};
        point p2 = {30, 40};

        DrawLine(p1, p2);
        return 0;
}
